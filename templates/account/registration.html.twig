{% extends 'base.html.twig' %}
{% block title %}Incription{% endblock %}
{% block body %}
    <div class="register-form-wrapper mbfooter">
        <div class="container d-flex flex-column">
            <div class=" card form-wapper">
                <div class="d-flex header-form">
                    <h3>Inscription</h3>
                </div>
                <div class="px-3">
                    {{form_start(form)}}
                    <div class="form-group mt-3">
                        {{form_row(form.firstName)}}
                    </div>
                    <div class="form-group">
                        {{form_row(form.lastName)}}
                    </div>
                    <div class="form-group">
                        {{form_row(form.email)}}
                    </div>
                    <div class="form-group">
                        {{form_row(form.avatar)}}
                    </div>
                    <div class="form-group">
                        {{form_row(form.hash)}}
                    </div>
                    <div class="form-group">
                        {{form_row(form.hash)}}
                    </div>
                    <div class="form-group">
                        {{form_row(form. passwordConfirme)}}
                    </div>
                    <div class="form-group">

                        <button type="submit" class="btn btn-success btnRegister">Confirmer l'inscription</button>

                    </div>

                    {{form_end(form)}}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
